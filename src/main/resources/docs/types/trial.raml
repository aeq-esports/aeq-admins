#%RAML 1.0 DataType

type: object
properties:
  status:
    enum: [ open, pending, rejected, accepted ]
  requiresReview: boolean
  start: datetime
  end: datetime
  votes:
    type: array
    items: !include trial-vote.raml
  reviewVotes:
    type: array
    items: !include trial-vote.raml
  closeVotes: integer